{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch MAUI (Windows)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build Finance.Mobile",
      "program": "${workspaceFolder}/src/Finance.Mobile/bin/Debug/net10.0-windows10.0.19041.0/win-x64/Finance.Mobile.exe",
      "args": [],
      "cwd": "${workspaceFolder}/src/Finance.Mobile",
      "console": "integratedTerminal",
      "stopAtEntry": false,
      "internalConsoleOptions": "openOnSessionStart",
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    {
      "name": "Attach to Finance.Mobile",
      "type": "coreclr",
      "request": "attach"
    }
  ]
}
